<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.projetcapteurs">
    <uses-feature android:name="android.hardware.sensor.light" android:required="true" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/icone"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icone_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ProjetCapteurs">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Gyroscope" />
        <activity android:name=".Luminosity" />
        <activity android:name=".Pressure" />
        <activity android:name=".Niveau" />
    </application>

</manifest>